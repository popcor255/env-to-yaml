{
    "terminal.integrated.shell.linux": "/usr/bin/bash",
    "go.gopath": "/usr/lib/go",
    "go.formatTool": "goformat",
    "go.autocompleteUnimportedPackages": true,
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    }
}